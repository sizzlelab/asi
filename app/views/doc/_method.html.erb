<h3><%= name %></h3>
<div class="method">
<p>
  <%= description %>
</p>


<% if parameters && parameters.size > 0 %>
<h4>Parameters</h4>
  <table>
  <% parameters.each do |parameter| %>
    <tr><th><%= parameter.to_a[0][0] %></th><td><%= parameter.to_a[0][1] %></td></tr>
  <% end %>
  </table>
<% end %>
</div>

